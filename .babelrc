{
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-classes",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-object-set-prototype-of-to-assign"
      ]
  
    ]
  }